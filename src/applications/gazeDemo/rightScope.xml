          gridy="0"
          hspan="1"
          vspan="1"
          size="201"
          minval="0"
          title="[Black] Right Event Rate (kHz) [Red] Filter Rate (Hz)"
          maxval="3200"
          bgcolor="#EEFFEE">
        <graph remote="/vpfR/scope:o"
               index="1"
               title="Filter Rate (Hz)"
